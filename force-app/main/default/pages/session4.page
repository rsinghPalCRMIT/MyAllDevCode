<apex:page standardController="contact" lightningStylesheets="true" extensions="extsession4">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection >
                <apex:inputField value="{!objContact.LastName}"/>
                <apex:inputField value="{!objContact.Phone}"/>
                <apex:inputField value="{!contact.Account.Name}"/>
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton value="save" action="{!save}"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
  
</apex:page>